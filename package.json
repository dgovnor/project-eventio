    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "7.13.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "jest-environment-enzyme": "^7.1.2",
